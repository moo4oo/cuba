<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.company.example.web.file.FileEdit"
        datasource="fileDs"
        focusComponent="fieldGroup"
        messagesPack="com.company.example.web.file">
    <dsContext>
        <datasource id="fileDs"
                    class="com.company.example.entity.File"
                    view="file-view"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="fileDs">
            <column width="250px">
                <field property="outDoc"/>
                <field property="file"/>
                <field property="create_time"/>
                <field property="update_time"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
