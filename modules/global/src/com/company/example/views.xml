<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">
    <view class="com.company.example.entity.SubDivision"
          extends="_local"
          name="subDivision-view">
        <property name="lead_subdivision"
                  view="_minimal">
        </property>
        <property name="departament_head"
                  view="_minimal">
            <property name="user"
                      view="_minimal"/>
        </property>
    </view>
    <view class="com.company.example.entity.Workers"
          extends="_local"
          name="workers-view">
        <property name="user"
                  view="_minimal">
            <property name="firstName"/>
            <property name="lastName"/>
            <property name="middleName"/>
        </property>
        <property name="photo"
                  view="_local"/>
        <property name="sub_division"
                  view="_minimal">
            <property name="lead_subdivision"
                      view="_minimal"/>
            <property name="departament_head"
                      view="_minimal">
                <property name="user"
                          view="_minimal">
                    <property name="firstName"/>
                    <property name="lastName"/>
                </property>
            </property>
            <property name="code"/>
        </property>
    </view>
    <view class="com.company.example.entity.Organizations"
          extends="_local"
          name="organizations-view">
        <property name="code"/>
        <property name="short_title"/>
        <property name="title"/>
        <property name="law_address"/>
        <property name="mail_address"/>
    </view>
    <view class="com.company.example.entity.RegistrationLogs"
          extends="_local"
          name="registrationLogs-view">
    </view>
    <view class="com.company.example.entity.AffairsNomenclature"
          extends="_local"
          name="affairsNomenclature-view">
        <property name="code"/>
        <property name="title"/>
    </view>
    <view class="com.company.example.entity.OutgoingDocuments"
          extends="_local"
          name="main-outgoingDocuments-view">
        <property name="document_type"
                  view="_minimal">
            <property name="code"/>
            <property name="serial_number"/>
        </property>
        <property name="addressee"
                  view="_minimal">
            <property name="title"/>
        </property>
        <property name="executor"
                  view="_minimal">
        </property>
        <property name="sign"
                  view="_minimal">
            <property name="user"
                      view="_minimal">
                <property name="firstName"/>
                <property name="lastName"/>
            </property>
        </property>
        <property name="author"
                  view="_minimal">
            <property name="firstName"/>
            <property name="lastName"/>
        </property>
        <property name="log"
                  view="_minimal">
            <property name="code"/>
            <property name="title"/>
            <property name="number"/>
            <property name="serial_number"/>
        </property>
        <property name="affair"
                  view="_minimal"/>
        <property name="file_des"
                  view="_local"/>
    </view>
    <view class="com.company.example.entity.OutgoingDocuments"
          extends="_local"
          name="documents-outgoingDocuments-view">
    </view>
    <view class="com.company.example.entity.OutgoingDocuments"
          extends="_local"
          name="matching-outgoingDocuments-view">
    </view>
    <view class="com.company.example.entity.MatchingTable"
          extends="_local"
          name="matchingTable-view">
        <property name="matcher"
                  view="_minimal">
            <property name="user"
                      view="_minimal">
                <property name="firstName"/>
                <property name="lastName"/>
            </property>
            <property name="sub_division"
                      view="_minimal"/>
        </property>
    </view>
    <view class="com.haulmont.bpm.entity.ProcInstance"
          extends="_local"
          name="procInstance-view-custom">
        <property name="procTasks"
                  view="_minimal">
            <property name="outcome"/>
            <property name="name"/>
            <property name="comment"/>
        </property>
    </view>
    <view class="com.haulmont.bpm.entity.ProcInstance"
          extends="_local"
          name="procInstance-full"
          overwrite="true">
        <property name="procDefinition"
                  view="_local">
            <property name="procRoles"
                      view="_local"/>
        </property>
        <property name="startedBy"
                  view="_minimal"/>
        <property name="entity"
                  view="_local"/>
        <property name="procActors"
                  view="_local">
            <property name="procRole"
                      view="_local"/>
            <property name="user"
                      view="_local"/>
        </property>
        <property name="procAttachments"
                  view="_local">
            <property name="type"
                      view="_minimal"/>
            <property name="file"
                      view="_local"/>
            <property name="author"
                      view="_minimal"/>
        </property>
        <property name="procTasks"
                  view="_local">
            <property name="procActor"
                      view="_minimal"/>
        </property>
    </view>
    <view class="com.haulmont.bpm.entity.ProcTask"
          extends="_local"
          name="procTask-view">
        <property name="procActor"
                  view="_local">
            <property name="procRole"
                      view="_local">
                <property name="procDefinition"
                          view="_minimal"/>
            </property>
            <property name="user"
                      view="_local"/>
            <property name="procInstance"
                      view="_local"/>
        </property>
        <property name="procInstance"
                  view="_local">
            <property name="procActors"
                      view="_local">
                <property name="user"
                          view="_minimal"/>
                <property name="procRole"
                          view="_local"/>
            </property>
            <property name="entity"
                      view="_local">
            </property>
            <property name="procAttachments"
                      view="_local"/>
        </property>
    </view>
    <view class="com.haulmont.bpm.entity.ProcActor"
          extends="_local"
          name="procActor-view">
        <property name="user"
                  view="_minimal"/>
        <property name="procInstance"
                  view="_minimal">
            <property name="entity"
                      view="_minimal">
                <property name="entityId"/>
            </property>
        </property>
        <property name="procRole"
                  view="_minimal"/>
    </view>
    <view class="com.company.example.entity.Workers"
          extends="_local"
          name="workers-view_1">
        <property name="user"
                  view="_minimal"/>
    </view>
    <view class="com.haulmont.bpm.entity.ProcTask"
          extends="_local"
          name="procTask-view_1">
        <property name="procActor"
                  view="_minimal"/>
        <property name="procInstance"
                  view="_minimal">
            <property name="entity"
                      view="_minimal">
                <property name="entityId"/>
            </property>
        </property>
    </view>
</views>
